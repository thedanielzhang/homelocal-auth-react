{
  "name": "@homelocal/auth",
  "version": "0.1.0",
  "description": "Reusable authentication library for Home.Local React applications",
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./axios": {
      "import": "./dist/axios.js",
      "types": "./dist/axios.d.ts"
    }
  },
  "files": [
    "dist",
    "README.md"
  ],
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "type-check": "tsc --noEmit",
    "lint": "eslint src --ext .ts,.tsx",
    "test": "vitest",
    "prepublishOnly": "npm run build"
  },
  "peerDependencies": {
    "axios": ">=1.0.0",
    "react": ">=18.0.0"
  },
  "devDependencies": {
    "@types/react": "^18.2.0",
    "axios": "^1.6.0",
    "react": "^18.2.0",
    "typescript": "^5.3.0",
    "vitest": "^1.0.0"
  },
  "keywords": [
    "homelocal",
    "auth",
    "jwt",
    "react",
    "authentication"
  ],
  "author": "Home.Local Platform Team",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/homelocal/homelocal-auth-react"
  }
}
